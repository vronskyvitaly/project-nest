{
  "version": 2,
  "name": "next-store-server",
  "buildCommand": "npm start",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}